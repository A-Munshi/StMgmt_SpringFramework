<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
 http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd">
    <!-- Bean with Constructor Injection -->
    <bean id="addressBean" class="com.example.Address">
        <constructor-arg value="Kolkata"/>
        <constructor-arg value="West Bengal"/>
        <constructor-arg value="India"/>
    </bean>
    <!-- Bean with Setter Injection -->
    <bean id="studentBean" class="com.example.Student">
        <property name="id" value="101"/>
        <property name="name" value="Anuvab"/>
        <property name="address" ref="addressBean"/>
    </bean>
</beans>
